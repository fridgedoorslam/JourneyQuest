<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carl2tre.journeyquest" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/jq_logo"
        android:label="@string/app_name"
        android:theme="@style/ThemeOverlay.AppCompat.Dark" >
        <activity
            android:name=".TripList"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".EventList"
            android:label="@string/title_activity_trip_options" >
        </activity>
        <activity
            android:name=".SplashScreen"
            android:label="@string/title_activity_splash_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TransportationEvent"
            android:label="@string/title_activity_transportation_event" >
        </activity>
        <activity
            android:name=".ReservationEvent"
            android:label="@string/title_activity_reservation_event" >
        </activity>
        <activity
            android:name=".CustomEvent"
            android:label="@string/title_activity_custom_event" >
        </activity>
    </application>

</manifest>
